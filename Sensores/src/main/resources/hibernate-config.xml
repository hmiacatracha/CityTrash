<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
	<session-factory>
					 
		<property name="hibernate.dialect">${hibernate.dialect}</property>
        <property name="hibernate.show_sql">${hibernate.show_sql}</property>
        <property name="hibernate.format_sql">${hibernate.format_sql}</property>
        <property name="hibernate.use_sql_comments">${hibernate.use_sql_comments}</property>
        
		<mapping class="es.udc.fic.tfg.sensores.model.dumpster.Dumpster;" />
		<mapping class="es.udc.fic.tfg.sensores.model.province.Province;" />
		<mapping class="es.udc.fic.tfg.sensores.model.record.Record" />
		<mapping class="es.udc.fic.tfg.sensores.model.sensor.Sensor" />
		<mapping class="es.udc.fic.tfg.sensores.model.townhall.TownHall" />
		<mapping class="es.udc.fic.tfg.sensores.model.type.Type" />

	</session-factory>
</hibernate-configuration>